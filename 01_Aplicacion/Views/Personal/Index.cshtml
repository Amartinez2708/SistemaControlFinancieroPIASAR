            </div>
            <div class="modal-body">
                <fieldset class="scheduler-border">
                    <legend class="scheduler-border">Contrato</legend>

                    <div class="row">
                        <div class="col-lg-12 col-sm-12">
                            <h4 class="sub-title">Datos del Personal</h4>
                            <div class="row">
                                <div class="col-lg-12 col-sm-12">
                                    <div class="form-group">
                                        <label>Personal</label>
                                        @Html.TextBox("txtPersonalContrato", null, new { @class = "form-control", @readonly = "readonly" })
                                    </div>
                                </div>
                                <div class="col-lg-12 col-sm-12">
                                    <div class="form-group">
                                        <label>Dirección</label>
                                        @Html.TextBox("txtDireccionContrato", null, new { @class = "form-control", @readonly = "readonly" })
                                    </div>
                                </div>
                            </div>
                            <h4 class="sub-title">Datos del Contrato</h4>
                            <div class="row">
                                <div class="col-lg-6 col-sm-12">
                                    <div class="form-group">
                                        <label>Tipo de Contrato*</label>
                                        <select name="ddlTipoContrato" id="ddlTipoContrato" class="form-control">
                                            <option value="0">[--Seleccione--]</option>
                                            <option value="1">Consultoria Individual</option>
                                            <option value="2">Selección Directa</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-12">
                                    <div class="form-group">
                                        <label>Nro. de Contrato*</label>
                                        @Html.TextBox("txtNroContrato", null, new { @class = "form-control"})
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-12">
                                    <div class="form-group">
                                        <label>Cargo*</label>
                                        @Html.DropDownList("ddlCargoContrato", new SelectList(ViewBag.ddlCargoContrato, "id", "text"), new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-12">
                                    <div class="form-group">
                                        <label>Oficina de Dependencia*</label>
                                        @Html.DropDownList("ddlOficinaDependencia", new SelectList(ViewBag.ddlOficinaDependencia, "id", "text"), new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="col-lg-12 col-sm-12">
                                    <div class="form-group">
                                        <label>Ejecución del Trabajo y Supervisión del Contrato*</label>
                                        @Html.TextArea("txtEjecucionTrabajoSupervision", null, new { @class = "form-control"})
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-12">
                                    <div class="form-group">
                                        <label>Fecha de Inicio*</label>
                                        @Html.TextBox("txtFechaInicio", null, new { @class = "form-control date-pick" })
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-12">
                                    <div class="form-group">
                                        <label>Fecha de Fin*</label>
                                        @Html.TextBox("txtFechaFin", null, new { @class = "form-control date-pick" })
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-12">
                                    <div class="form-group">
                                        <label>Lugar de Prestacion de Servicios*</label>
                                        @Html.DropDownList("ddlLugarPrestacionServicios", new SelectList(ViewBag.ddlLugarPrestacionServicios, "id", "text"), new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-12">
                                    <div class="form-group">
                                        <label>Monto de Contrato*</label>
                                        @Html.TextBox("txtMontoContrato", null, new { @class = "form-control", @onkeypress = "return soloDecimales(event, this);" })
                                    </div>
                                </div>
                                <div class="col-lg-12 col-sm-12">
                                    <div class="form-group">
                                        <label>Forma de Pago*</label>
                                        @Html.TextArea("txtFormaPago", null, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="col-lg-12 col-sm-12">
                                    <div class="form-group">
                                        <label>Representante Legal*</label>
                                        @Html.DropDownList("ddlRepresentanteLegal", new SelectList(ViewBag.ddlRepresentanteLegal, "id", "text"), new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>

                    <div class="form-group text-right">
                        <button id="btnGuardarContrato" type="button" class="btn btn-primary waves-effect " onclick="GuardarContrato()"><i class="fa fa-save" aria-hidden="true"></i>Guardar</button>
                    </div>
                </fieldset>
            </div>
            <div class="modal-footer text-center">
                <button type="button" class="btn btn-danger waves-effect " onclick="CerrarContrato()"><i class="fa fa-times" aria-hidden="true"></i>Cerrar</button>
            </div>
        </div>
    </div>
</div>

