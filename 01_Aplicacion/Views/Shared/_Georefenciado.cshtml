<body style="width: 100%;height: 100%;margin: 0;padding: 0;overflow-y: hidden;">
    <div class="theme-loader">
        <div class="loader-track">
            <div class="loader-bar"></div>
        </div>
    </div>
    <div id="pcoded" class="pcoded">
        <div class="pcoded-container">
            <nav class="navbar header-navbar pcoded-header" style="background-image: repeating-linear-gradient(to bottom right, #bf0909 0%, #c24d67 100%);">
                <div class="navbar-wrapper">
                    <div class="navbar-logo">
                        <a class="mobile-menu" id="mobile-collapse" href="#!">
                            <i class="ti-menu"></i>
                        </a>
                        <a href="@Url.Action("Index")">
                            <img class="img-fluid" src="~/Content/images/pnsr_logo.png" alt="Theme-Logo" />
                        </a>
                        <a class="mobile-options">
                            <i class="ti-more"></i>
                        </a>
                    </div>
                    <div class="navbar-container container-fluid">
                        <ul class="nav-left">
                            <li>
                                <div class="sidebar_toggle">
                                    <a href="javascript:void(0)"><i class="ti-menu"></i></a>
                                </div>
                            </li>
                            <li>
                                <a href="#!" onclick="javascript:toggleFullScreen()">
                                    <i class="ti-fullscreen"></i>
                                </a>
                            </li>

                        </ul>
                        <ul class="nav-left">
                            <li class="user-profile header-notification">
                                <a><span>Sistema de Monitoreo y Gestión de Proyectos PIASAR</span></a>
                            </li>
                        </ul>
                        <ul class="nav-right d-none">
                            <li class="user-profile header-notification">
                                <a class="btn btn-danger btn-round p-6" href="/Login" style="padding:6px;">
                                    <i class="fa fa-sign-in" aria-hidden="true" style="font-size:16px;"></i>
                                    <span id="usu">Acceder &nbsp;</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
            <div class="pcoded-main-container">
                <div class="pcoded-wrapper">
                    <div class="main-body img-paper" style="height:100vh;">
                        <div class="page-wrapper p-0">
                            <div class="page-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
