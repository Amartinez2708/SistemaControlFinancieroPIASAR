<body>

    <div class="theme-loader">
        <div class="loader-track">
            <div class="loader-bar"></div>
        </div>
    </div>

    <div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">
            <nav class="navbar header-navbar pcoded-header" style="background-image: repeating-linear-gradient(to bottom right, #bf0909 0%, #c24d67 100%);">
                <div class="navbar-wrapper">
                    <div class="navbar-logo">
                        <a class="mobile-menu" id="mobile-collapse" href="#!">
                            <i class="ti-menu"></i>
                        </a>
                        <a href="@Url.Action("Index")">
                            <img class="img-fluid" src="~/Content/images/pnsr_logo.png" alt="Theme-Logo" />
                        </a>
                        <a class="mobile-options">
                            <i class="ti-more"></i>
                        </a>
                    </div>
                    <div class="navbar-container container-fluid">
                        <ul class="nav-left">
                            <li>
                                <div class="sidebar_toggle">
                                    <a href="javascript:void(0)"><i class="ti-menu"></i></a>
                                </div>
                            </li>
                        </ul>
                        <ul class="nav-left">
                            <li class="user-profile header-notification" style="line-height: 2;">
                                <a style="font-size:28px"><span>Sistema de Control Financiero</span></a>
                            </li>
                        </ul>
                        <ul class="nav-right">
                            @{ Html.RenderAction("UserInfo", "Inicio");}
                        </ul>
                    </div>
                </div>
            </nav>

            <div class="pcoded-main-container img-paper">

                <!--NavBar-->
                <nav class="pcoded-navbar">
                    <div class="sidebar_toggle"><a href="#"><i class="icon-close icons"></i></a></div>
                    <div class="pcoded-inner-navbar main-menu">
                        @{ Html.RenderAction("SideMenu", "Inicio");}
                    </div>
                </nav>
                <!--End NavBar-->

                <div class="pcoded-wrapper">

                    <div class="pcoded-content">
                        <div class="pcoded-inner-content">

                            <div class="main-body">
                                <div class="page-wrapper">
                                    <div class="page-body">
                                        <div class="row">
                                            @RenderBody()
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


