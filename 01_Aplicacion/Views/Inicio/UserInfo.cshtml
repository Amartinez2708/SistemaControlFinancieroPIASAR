
<li class="user-profile header-notification">
    <a href="#!">
        <img src="@ViewBag.ImgenUsuario" class="img-radius" alt="User-Profile-Image" />

        <span id="usu" class="FadeNameUser">@ViewBag.UsuarioNombre </span>
        <span id="perfil" class="FadeNamePerfil">[ @ViewBag.PerfilUsuario ]</span>
        <i class="ti-angle-down"></i>
    </a>
    <ul class="show-notification profile-notification">
        <li>
            <a href="@Url.Action("Index", "Login")">
                <i class="fa fa-sign-out" aria-hidden="true"></i> Cerrar Sesión
            </a>
        </li>
    </ul>
</li>